{
  "multipart": [
    {
      "when": {
        "north": "input"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_input"
      }
    },
    {
      "when": {
        "north": "output"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_output"
      }
    },
    {
      "when": {
        "north": "pipe"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_side"
      }
    },
    {
      "when": {
        "east": "input"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_input",
        "y": 90
      }
    },
    {
      "when": {
        "east": "output"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_output",
        "y": 90
      }
    },
    {
      "when": {
        "east": "pipe"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "input"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_input",
        "y": 180
      }
    },
    {
      "when": {
        "south": "output"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_output",
        "y": 180
      }
    },
    {
      "when": {
        "south": "pipe"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "input"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_input",
        "y": 270
      }
    },
    {
      "when": {
        "west": "output"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_output",
        "y": 270
      }
    },
    {
      "when": {
        "west": "pipe"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "input"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_input",
        "x": 270
      }
    },
    {
      "when": {
        "up": "output"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_output",
        "x": 270
      }
    },
    {
      "when": {
        "up": "pipe"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_side",
        "x": 270
      }
    },
    {
      "when": {
        "down": "input"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_input",
        "x": 90
      }
    },
    {
      "when": {
        "down": "output"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_output",
        "x": 90
      }
    },
    {
      "when": {
        "down": "pipe"
      },
      "apply": {
        "model": "geoe:block/pipe/geothermal_pipe_side",
        "x": 90
      }
    },
    {
      "when": {
        "north": "none"
      },
      "apply": [
        {
          "model": "geoe:block/pipe/geothermal_pipe_noside",
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "east": "none"
      },
      "apply": [
        {
          "model": "geoe:block/pipe/geothermal_pipe_noside",
          "y": 90,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "south": "none"
      },
      "apply": [
        {
          "model": "geoe:block/pipe/geothermal_pipe_noside",
          "y": 180,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "west": "none"
      },
      "apply": [
        {
          "model": "geoe:block/pipe/geothermal_pipe_noside",
          "y": 270,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "up": "none"
      },
      "apply": [
        {
          "model": "geoe:block/pipe/geothermal_pipe_noside",
          "x": 270,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "down": "none"
      },
      "apply": [
        {
          "model": "geoe:block/pipe/geothermal_pipe_noside",
          "x": 90,
          "uvlock": true
        }
      ]
    }
  ]
}